{
  "name": "SimpleAdRotator",
  "description": "Mobile Ad",
  "version": "1.0.0",
  "author": {
    "name": "Pressly",
    "url": "http://www.pressly.com/"
  },
  "homepage": "http://www.pressly.com/",
  "dependencies": {
    "browserify": "^3.33.0",
    "browserify-shim": "^3.3.2",
    "gulp-rename": "^1.2.0",
    "gulp-browserify": "^0.5.0",
    "coffeeify": "^0.6.0"
  },
  "devDependencies": {
    "gulp": "3.5.5",
    "coffee-script": "1.7.1",
    "gulp-coffee": "1.4.1",
    "gulp-stylus": "0.0.14",
    "gulp-watch": "0.5.2",
    "path": "0.4.9",
    "gulp-connect": "1.0.7"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ],
    "extensions": [
      ".coffee"
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/jquery.js",
    "handlebars": "./bower_components/handlebars/handlebars.js",
    "ember": "./bower_components/ember/ember.js",
    "ember_data": "./bower_components/ember-data/ember-data.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "handlebars": "Handlebars",
    "ember": {
      "depends": [
        "jquery:$",
        "handlebars:Handlebars"
      ],
      "exports": "Ember"
    },
    "ember_data": {
      "depends": [
        "ember:Ember"
      ],
      "exports": "DS"
    }
  }
}
